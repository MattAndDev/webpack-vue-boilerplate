{
  "name": "webbpack-vue-boilerplate",
  "version": "0.0.1",
  "description": "kickstart a vue project with a lot of sugar",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "NODE_ENV=development npm-run-all build-watch start-watch",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config .webpack/webpack.config.js",
    "build-watch": "NODE_ENV=production ./node_modules/.bin/webpack --display errors-only --config .webpack/webpack.config.js",
    "start-watch": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config .webpack/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MattAndDev/on-a-trip"
  },
