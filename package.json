    "url": "https://mattinthe.net"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "vue": "^2.4.2",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.7.0"
  },
